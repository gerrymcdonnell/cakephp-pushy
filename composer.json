{
    "name": "gerrymcdonnell/pushy",
    "description": "Gerrymcdonnell/Pushy plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Gerrymcdonnell\\Pushy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerrymcdonnell\\Pushy\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
